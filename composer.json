{
	"keywords" : [
		"php",
		"formatter",
		"beautifier"
	],
	"autoload" : {
		"psr-4" : {
			"gossi\\formatter\\" : "src"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"gossi\\formatter\\tests\\" : "tests"
		}
	},
	"name" : "gossi/php-code-formatter",
	"type" : "library",
	"description" : "a code formatter for php code",
	"license" : "MIT",
	"require" : {
		"php" : ">=7.0.1 || >=5.5",
		"symfony/config" : "~3",
		"symfony/yaml" : "~3",
		"gossi/collection" : "~1",
		"symfony/event-dispatcher" : "~3"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~5"
	}
}
